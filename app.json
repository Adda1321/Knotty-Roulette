{
  "expo": {
    "name": "knotty-roulette",
    "slug": "knotty-roulette",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/knotty-roulette.png",
    "scheme": "knottyroulette",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/knotty-roulette.png",
        "backgroundColor": "#00cc00"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.adil_mustafa.knottyroulette"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/knotty-roulette.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/knotty-roulette.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#00cc00"
        }
      ],
      "expo-audio"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "395318b6-852c-4887-9320-7b7b4740d182"
      }
    },
    "owner": "adil_mustafa"
  }
}
