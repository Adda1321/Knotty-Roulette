      "adaptiveIcon": {
        "foregroundImage": "./assets/images/knotty-roulette.png",
        "backgroundColor": "#00cc00"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "package": "com.adilmustafa.knottyroulette"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/knotty-roulette.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/knotty-roulette.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#00cc00"
